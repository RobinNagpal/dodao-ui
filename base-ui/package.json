{
  "name": "base-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "compile": "prisma generate && tsc",
    "dev": "prisma generate && next dev -p 3003",
    "build": "prisma generate && next build",
    "start": "prisma generate && next start -p 3003",
    "lint": "next lint",
    "lint-fix": "eslint . --ext .ts,.js,.tsx --fix",
    "prettier-fix": "prettier --config .prettierrc 'src/**/*.{js,ts,tsx,json}' --write",
    "prettier-check": "prettier --check 'src/**/*.{js,ts,tsx,json}'",
    "graphql:download": "graphql-codegen --config introspection.yml",
    "graphql:generate": "graphql-codegen --config codegen.yml"
  },
  "engines": {
    "node": ">=23.11.0"
  },
  "dependencies": {
    "@apollo/client": "^3.7.12",
    "autoprefixer": "10.4.14",
    "@dodao/web-core": "workspace:*",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "6.19.0",
    "ethers": "^6.3.0",
    "graphql": "^16.8.1",
    "@heroicons/react": "2.1.5",
    "next": "15.5.7",
    "next-auth": "^4.24.11",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sass": "^1.62.1",
    "styled-components": "6.1.11",
    "lodash": "^4.17.21",
    "@tailwindcss/forms": "^0.5.3",
    "tailwindcss": "3.4.17",
    "typescript": "5.0.4",
    "uuid": "^9.0.0",
    "@vercel/analytics": "^1.0.1",
    "@headlessui/react": "^2.2.9",
    "react-ga4": "^2.1.0"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.26",
    "@types/node": "18.16.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/lodash": "^4.14.194",
    "@types/connect": "^3.4.38",
    "@types/trusted-types": "^2.0.7",
    "@types/ws": "^8.5.10",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.4",
    "postcss": "8.4.35",
    "prisma": "6.19.0",
    "prettier": "^2.8.8",
    "prisma-json-types-generator": "3.1.1",
    "@types/uuid": "^9.0.1"
  }
}
