{
  "name": "remotion-automated",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "lint": "eslint src --ext .ts",
    "lint-fix": "eslint src --ext .ts --fix",
    "prettier-fix": "prettier --config .prettierrc 'src/**/*.{js,ts,json}' --write",
    "prettier-check": "prettier --check 'src/**/*.{js,ts,json}'",
    "test:single-slide": "npx tsx scripts/test-single-slide.ts",
    "test:concatenate": "npx tsx scripts/test-concatenate.ts",
    "deploy": "npx serverless deploy",
    "deploy:prod": "npx serverless deploy --stage prod"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/s3-request-presigner": "^3.709.0",
    "@remotion/bundler": "4.0.398",
    "@remotion/cli": "4.0.398",
    "@remotion/media-parser": "4.0.398",
    "@remotion/renderer": "4.0.398",
    "@remotion/tailwind-v4": "4.0.398",
    "aws-lambda": "^1.0.7",
    "dotenv": "^17.2.3",
    "edge-tts-universal": "^1.3.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "remotion": "4.0.398",
    "tailwindcss": "^4.0.0",
    "typescript": "5.0.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "18.16.3",
    "@types/react": "^19.2.7",
    "eslint": "8.39.0",
    "prettier": "^2.8.8",
    "serverless": "^4.21.0",
    "tsx": "^4.20.6",
    "typescript": "5.0.4"
  }
}
