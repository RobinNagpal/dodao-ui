# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key

# S3 Bucket for assets
S3_BUCKET_NAME=your-s3-bucket-name

# Remotion Lambda Configuration
# Run 'npm run setup:remotion' to get these values
REMOTION_APP_REGION=us-east-1
REMOTION_APP_FUNCTION_NAME=remotion-render-xxxxx
REMOTION_APP_SERVE_URL=https://s3.us-east-1.amazonaws.com/bucket-name/sites/xxxxx/
