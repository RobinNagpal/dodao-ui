{
  "name": "clickable-demos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "compile": "tsc",
    "build:scss": "sass --no-source-map src/styles/clickableDemoTooltipStyles.scss prod-files/clickableDemoTooltipStyles.css",
    "build": "cd ../shared/web-core && yarn build && cd ../../clickable-demos && tsc && npm run build:scss",
    "dev": "tsc -w",
    "lint": "next lint",
    "lint-fix": "eslint . --ext .ts,.js,.tsx --fix",
    "prettier-fix": "prettier --config .prettierrc 'src/**/*.{js,ts,tsx,json}' --write",
    "prettier-check": "prettier --check 'src/**/*.{js,ts,tsx,json}'",
    "test-server": "http-server -o --cors -d sample-demos -p 9090"
  },
  "dependencies": {
    "@dodao/web-core": "1.0.0",
    "sass": "^1.62.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/html2canvas": "^1.0.0",
    "@types/node": "18.16.3",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.4",
    "html2canvas": "^1.4.1",
    "http-server": "^14.1.1",
    "postcss": "8.4.35",
    "prettier": "^2.8.8",
    "tippy.js": "^6.3.7",
    "typescript": "5.0.4"
  }
}
